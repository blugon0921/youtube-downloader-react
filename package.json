{
  "name": "youtube_downloader",
  "version": "1.0.11",
  "author": "Blugon",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@distube/ytdl-core": "^4.13.7",
    "@electron/remote": "^2.1.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "downloads-folder": "^3.0.3",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^5.0.3",
    "electron-progressbar": "^2.1.0",
    "electron-updater": "^6.1.7",
    "ffmpeg-static-electron": "^2.0.3",
    "hidefile": "^3.0.0",
    "node-fetch": "^3.3.2",
    "progress-estimator": "^0.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-modal": "^3.16.1",
    "react-scripts": "5.0.1",
    "react-select": "^5.8.0",
    "styled-components": "^6.1.6",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "electron": "31.2.0",
    "electron-builder": "^24.9.1"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "start": "concurrently \"cross-env NODE_ENV=development BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "react-build": "react-scripts build",
    "build": "npm run react-build && electron-builder build -c.extraMetadata.main=build/electron.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blugon0921/youtube-downloader-react"
  },
  "build": {
    "productName": "Youtube Downloader",
    "appId": "kr.blugon.youtubedownloader",
    "publish": {
      "provider": "github",
      "owner": "blugon0921",
      "repo": "youtube-downloader-react",
      "releaseType": "release"
    },
    "asar": true,
    "mac": {
      "target": [
        "default"
      ],
      "icon": "build/icon.icns"
    },
    "dmg": {
      "title": "Youtube Downloader",
      "icon": "build/icon.icns"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "build/favicon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": false,
      "createDesktopShortcut": false,
      "perMachine": true,
      "language": 1042,
      "installerIcon": "build/favicon.ico",
      "shortcutName": "Youtube Downloader",
      "artifactName": "Youtube_Downloader-${version}.${ext}",
      "uninstallDisplayName": "Youtube Downloader"
    },
    "directories": {
      "buildResources": "./build/",
      "output": "./dist/"
    }
  }
}
